<p id="notice"><%= notice %></p>

<h1>Prospectts</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last address</th>
      <th>Mail</th>
      <th>Company</th>
      <th>Job title</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prospectts.each do |prospectt| %>
      <tr>
        <td><%= prospectt.first_name %></td>
        <td><%= prospectt.last_address %></td>
        <td><%= prospectt.mail %></td>
        <td><%= prospectt.company %></td>
        <td><%= prospectt.job_title %></td>
        <td><%= prospectt.phone %></td>
        <td><%= link_to 'Show', prospectt %></td>
        <td><%= link_to 'Edit', edit_prospectt_path(prospectt) %></td>
        <td><%= link_to 'Destroy', prospectt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prospectt', new_prospectt_path %>

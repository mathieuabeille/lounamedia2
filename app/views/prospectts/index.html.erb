<!-- <p id="notice"><%= notice %></p> -->
<center>
  <h1>Tableau de bord</h1>

  <table>
    <thead>
      <tr>
        <th>Prénom</th>
        <th>Nom</th>
        <th>Email</th>
        <th>Entreprise</th>
        <th>Poste</th>
        <th>Téléphone</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @prospectts.each do |prospectt| %>
        <tr>
          <td><%= prospectt.first_name %></td>
          <td><%= prospectt.last_name %></td>
          <td><%= prospectt.mail %></td>
          <td><%= prospectt.company %></td>
          <td><%= prospectt.job_title %></td>
          <td><%= prospectt.phone %></td>
          <td><%= link_to 'Edit', edit_prospectt_path(prospectt) %></td>
          <td><%= link_to 'Destroy', prospectt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</center>

